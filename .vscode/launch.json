{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "excited_state Be  ",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "/gpfs/projects/rjh/adrian/test-suite/outputs/hf_canon/10_Be/excited_state/r_input"
            ],
            "stopAtEntry": false,
            "cwd": "/gpfs/projects/rjh/adrian/test-suite/outputs/hf_canon/10_Be/excited_state",
            "environment": [
                {
                    "name": "PATH",
                    "value": "$PATH:${command:cmake.launchTargetDirectory}"
                },
                {
                    "name": "MAD_NUM_THREADS",
                    "value": "8"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}